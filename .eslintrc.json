{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/prefer-stateless-function": "off",
    "linebreak-style": 0,
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4, {"props": 4}]
  }
}
